{
    "name": "zgetro/laravel-dusk-chromedriver",
    "description": "A Laravel Artisan command to download matching ChromeDriver binaries for Windows, Linux, Mac, with WSL host detection support.",
    "type": "library",
    "license": "MIT",
    "keaywords": [
        "laravel",
        "dusk",
        "chromedriver",
        "webdriver",
        "wsl-Windows",
        "Windows",
        "Linux",
        "Mac"
    ],
    "authors": [
        {
            "name": "Zgetro",
            "email": "chirag7004classic@gmail.com",
            "homepage": "https://github.com/zgetro/laravel-dusk-chromedriver",
            "photo": "https://avatars.githubusercontent.com/u/4480849?v=4&size=64"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "laravel/dusk": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.5 || ^9.0"
    },
    "suggest": {
        "illuminate/support": "Required for Laravel integration",
        "illuminate/console": "Required for Artisan command",
        "laravel/dusk": "Required for Dusk integration"
    },
    "autoload": {
        "psr-4": {
            "DuskChromeDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DuskChromeDriver\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DuskChromeDriver\\DuskChromeDriverServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
